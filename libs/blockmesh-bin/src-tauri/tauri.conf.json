    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDgwNzZDOUI3ODlDNDE5NTAKUldSUUdjU0p0OGwyZ0oyS3B1YmJyeCtXb0ZOVktlU2RFdnpGc0V6VUdDcVFvQWN3cmNGditZQ20K",
